<section class='hints'>

  <input id="toggle-hints" type="checkbox" style="display: none; visibility: hidden;" unchecked>
    <label for="toggle-hints"></label>
  </input>

  <div id="expand-hints">
      <p></p>
        <div class="wrap">
          <table>
            <th>Error</th>
            <th>Possible Resolution</th>
            <tr>
              <td class='error'>"pg_restore: [archiver (db)] could not execute query: ERROR:  unrecognized configuration parameter "row_security"</td>
              <td>Upgrade your version of PostgreSQL to <%= ENV['DB_VERSION'] %>.</td>
            </tr>
          </table>
       </div>
      </p>
  </div>

</section>
